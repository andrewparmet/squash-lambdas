<configuration>
  <google>
    <cal>
      <creds>
        <location>s3</location>
      </creds>

      <calendarId>primary</calendarId>
    </cal>
  </google>

  <aws>
    <google>
      <cal>
        <creds>
          <bucket>${env:GOOGLE_CAL_CREDS_BUCKET}</bucket>
          <key>${env:GOOGLE_CAL_CREDS_KEY}</key>
        </creds>
      </cal>
    </google>

    <clubLocker>
      <creds>
        <bucket>${env:CLUB_LOCKER_CREDS_BUCKET}</bucket>
        <key>${env:CLUB_LOCKER_CREDS_KEY}</key>
      </creds>
    </clubLocker>

    <schedule>
      <bucket>${env:RESERVATION_SCHEDULE_BUCKET}</bucket>
      <key>${env:RESERVATION_SCHEDULE_KEY}</key>
    </schedule>

    <sns>
      <handledTopicArn>${env:HANDLED_TOPIC_ARN}</handledTopicArn>
    </sns>

    <dynamo>
      <squashSlotsTableName>${env:SLOTS_MONITORING_TABLE}</squashSlotsTableName>
      <squashSlotsUsersTableName>${env:SLOTS_MONITORING_USERS_TABLE}</squashSlotsUsersTableName>
      <squashSlotsUsersDailyPrefsTableName>${env:SLOTS_MONITORING_USERS_DAILY_PREFS_TABLE}</squashSlotsUsersDailyPrefsTableName>
    </dynamo>

    <pinpoint>
      <applicationId>${env:SLOTS_MONITORING_PINPOINT_APPLICATION_ID}</applicationId>
    </pinpoint>
  </aws>

  <clubLocker>
    <creds>
      <location>s3</location>
    </creds>
  </clubLocker>

  <schedule>
    <location>s3</location>
  </schedule>
</configuration>
